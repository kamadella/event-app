service firebase.storage {
  match /b/{bucket}/o {
    match /eventImages/{document}{
      allow read: if request.time < timestamp.date(2023, 10, 12);
      allow write: if request.auth != null;
    }
    match /userProfileImages/{document}{
      allow read: if request.time < timestamp.date(2023, 10, 12);
      allow write: if request.auth != null;
    }
  }
}
